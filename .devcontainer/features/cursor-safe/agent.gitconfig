[core]
    pager = cat
    autocrlf = input
    filemode = false

[init]
    defaultBranch = master

[push]
    default = simple

[pull]
    rebase = false

[color]
    ui = auto

[alias]
    st = status
    co = checkout
    br = branch
    ci = commit
    unstage = reset HEAD --
    last = log -1 HEAD
    visual = !code
